### Welcome to Uni-Plaza API
GET http://localhost:4000/

### Students API
# Get all students
GET http://localhost:4000/api/students

# Get student by ID
GET http://localhost:4000/api/students/{{studentId}}

# Create new student
POST http://localhost:4000/api/students
Content-Type: application/json

{
  "username": "john_doe",
  "email": "john@example.com",
  "password": "password123",
  "mobileNumber": 1234567890
}

# Update student
PUT http://localhost:4000/api/students/{{studentId}}
Content-Type: application/json

{
  "username": "john_updated",
  "email": "john.updated@example.com",
  "mobileNumber": 9876543210
}

# Delete student
DELETE http://localhost:4000/api/students/{{studentId}}

### Club Events API
# Get all club events
GET http://localhost:4000/api/clubs/events

# Get club event by ID
GET http://localhost:4000/api/clubs/events/{{eventId}}

# Create new club event
POST http://localhost:4000/api/clubs/events
Content-Type: application/json

{
  "clubname": "Tech Club",
  "title": "Hackathon 2024",
  "description": "Annual coding competition",
  "images": ["image1.jpg", "image2.jpg"],
  "date": "2024-03-15T10:00:00.000Z",
  "location": "Main Campus",
  "links": [
    {
      "label": "Registration",
      "link": "https://example.com/register"
    }
  ],
  "contact": {
    "email": "techclub@university.edu",
    "phoneNum": 1234567890
  },
  "universityName": "University of Technology",
  "tags": ["coding", "competition", "hackathon"]
}

# Update club event
PUT http://localhost:4000/api/clubs/events/{{eventId}}
Content-Type: application/json

{
  "title": "Updated Hackathon 2024",
  "description": "Updated description"
}

# Delete club event
DELETE http://localhost:4000/api/clubs/events/{{eventId}}

